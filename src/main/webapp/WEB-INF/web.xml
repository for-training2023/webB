<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	version="4.0">

	
	<error-page>
    <error-code>404</error-code>
    <location>/ja/404.jsp</location>
    </error-page>

	<servlet>
    <servlet-name>S00005</servlet-name>
    <servlet-class>jp.excd.servlet.S00005</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>S00005</servlet-name>
    <url-pattern>/ja/S00005</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>S00005_2</servlet-name>
    <servlet-class>jp.excd.servlet.S00005</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>S00005</servlet-name>
    <url-pattern>/ja/S00005/*</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>S00006</servlet-name>
    <servlet-class>jp.excd.servlet.S00006</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>S00006</servlet-name>
    <url-pattern>/ja/S00006/*</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>S00006change</servlet-name>
    <servlet-class>jp.excd.servlet.S00006</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>S00006change</servlet-name>
    <url-pattern>/ja/S00006/change</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>S00006back</servlet-name>
    <servlet-class>jp.excd.servlet.S00006</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>S00006back</servlet-name>
    <url-pattern>/ja/S00006/back</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>S00003</servlet-name>
    <servlet-class>jp.excd.servlet.S00003</servlet-class>
    <init-param>
      <param-name>javaEncoding</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>S00003</servlet-name>
    <url-pattern>/ja/S00003/*</url-pattern>
  </servlet-mapping>

</web-app>
